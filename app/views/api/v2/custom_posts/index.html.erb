<p id="notice"><%= notice %></p>

<h1>Api V2 Custom Posts</h1>

<table>
  <thead>
    <tr>
      <th>Status</th>
      <th>Content</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @api_v2_custom_posts.each do |api_v2_custom_post| %>
      <tr>
        <td><%= api_v2_custom_post.status %></td>
        <td><%= api_v2_custom_post.content %></td>
        <td><%= link_to 'Show', api_v2_custom_post %></td>
        <td><%= link_to 'Edit', edit_api_v2_custom_post_path(api_v2_custom_post) %></td>
        <td><%= link_to 'Destroy', api_v2_custom_post, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Api V2 Custom Post', new_api_v2_custom_post_path %>
